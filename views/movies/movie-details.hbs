<h2>Movie Details</h2>
<div>
    <h3>{{movie.title}}</h3>
        <br>
        <h4>{{movie.genre}}</h4>
        <h6>{{movie.plot}}</h6>
        <h5>Cast:</h5>
        <ul>
            {{#each movie.cast}}
            <li>{{this.name}}</li>
                <ul>
                    <li>{{this.occupation}}</li>
                    <li>{{this.catchPhrase}}</li>
                </ul>
            <br>
            {{/each}}
        </ul>
</div>
